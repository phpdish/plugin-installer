{
  "name": "phpdish/plugin-installer",
  "description": "A composer installer for installing PHPDish plugin.",
  "homepage": "https://www.phpdish.com",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Slince",
      "email": "taosikai@yeah.net"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "composer-plugin-api": "^1.0",
    "symfony/finder": "^2.0|^3.0|^4.0"
  },
  "require-dev": {
    "composer/composer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "PHPDish\\PluginInstaller\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPDish\\PluginInstaller\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "PHPDish\\PluginInstaller\\PluginInstaller"
  }
}